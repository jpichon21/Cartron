<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Site\CartronBundle\Entity\SousMenuDegustation" table="sous_menu_degustation">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="titreFr" type="string" column="titre_fr" length="255" nullable="true"/>
    <field name="descriptionFr" type="text" column="description_fr" nullable="true"/>
    <field name="noteDegustationFr" type="text" column="note_degustation_fr" nullable="true"/>
    <field name="titreEn" type="string" column="titre_en" length="255" nullable="true"/>
    <field name="descriptionEn" type="text" column="description_en" nullable="true"/>
    <field name="noteDegustationEn" type="text" column="note_degustation_en" nullable="true"/>
    <field name="image" type="string" column="image" length="255" nullable="true"/>
    <field name="couleurArrierePlan" type="string" column="couleur_arriere_plan" length="200" nullable="true"/>
    <many-to-one field="idDegustation" target-entity="Degustation">
      <join-columns>
        <join-column name="id_degustation" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
