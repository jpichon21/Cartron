<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Site\CartronBundle\Entity\SousMenu" table="sous_menu">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="titreFr" type="string" column="titre_fr" length="255" nullable="true"/>
    <field name="descriptionFr" type="text" column="description_fr" nullable="true"/>
    <field name="noteDegustationFr" type="text" column="note_degustation_fr" nullable="true"/>
    <field name="elaborationFr" type="text" column="elaboration_fr" nullable="true"/>
    <field name="contenanceFr" type="string" column="contenance_fr" length="255" nullable="true"/>
    <field name="conserverFr" type="string" column="conserver_fr" length="255" nullable="true"/>
    <field name="degre" type="integer" column="degre" nullable="true"/>
    <field name="image" type="string" column="image" length="255" nullable="true"/>
    <field name="icone" type="string" column="icone" length="255" nullable="true"/>
    <field name="titreEn" type="string" column="titre_en" length="255" nullable="true"/>
    <field name="descriptionEn" type="text" column="description_en" nullable="true"/>
    <field name="noteDegustationEn" type="text" column="note_degustation_en" nullable="true"/>
    <field name="elaborationEn" type="text" column="elaboration_en" nullable="true"/>
    <field name="contenanceEn" type="string" column="contenance_en" length="255" nullable="true"/>
    <field name="conserverEn" type="string" column="conserver_en" length="255" nullable="true"/>
    <field name="recetteFr" type="string" column="recette_fr" length="255" nullable="true"/>
    <field name="recetteEn" type="string" column="recette_en" length="255" nullable="true"/>
    <many-to-one field="idCollection" target-entity="Collection">
      <join-columns>
        <join-column name="id_collection" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
