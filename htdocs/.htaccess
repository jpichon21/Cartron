SetEnv REGISTER_GLOBALS 0
SetEnv ZEND_OPTIMIZER 1
SetEnv MAGIC_QUOTES 1
#SetEnv PHP_VER 5_3
<IfModule mod_rewrite.c>
    RewriteEngine On
	RewriteRule "(^|/)\." - [F]
    #<IfModule mod_vhost_alias.c>
    #    RewriteBase /
    #</IfModule>

    RewriteCond %{REQUEST_FILENAME} !-f
	#RewriteCond %{REQUEST_URI} !=/
    RewriteRule ^(.*)$ app.php [QSA,L]
</IfModule>
